ESPSection:NewButton("Treasure Chest ESP", "Shows the treasure chest", function()


            for __,v in pairs(game.workspace.Carriables:GetDescendants()) do -- the path 
 if v.Name == "Treasure hunt board_TreasureHunt1" then -- the itemgame:GetService("Workspace").Carriables["Treasure hunt board_TreasureHunt3"].MeshPart
   local a = Instance.new("BillboardGui",v) -- ui
   a.Name = "Box"
   a.Size = UDim2.new(1,0, 1,0)
   a.AlwaysOnTop = true
   local b = Instance.new("Frame",a)
   b.Size = UDim2.new(1,0, 1,0)
   b.BackgroundTransparency = 0.80
   b.BorderSizePixel = 0
   b.Transparency = 1
   b.BackgroundColor3 = Color3.new(255, 0, 0)
   local c = Instance.new('TextLabel',b)
   c.Size = UDim2.new(1,0,0.1,0)
