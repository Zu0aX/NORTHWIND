            local function constantset(func_name, script_name)
    local f = nil
    for i, k in pairs(getgc()) do
        if f ~= nil then 
            break
        end
        if typeof(k) == "function" and islclosure(k) then
            local info = debug.getconstants(k)
            local SHORT_SRC = tostring(debug.getinfo(k).short_src)
            for _,v in pairs(info) do
                if tostring(debug.getinfo(k).name) == func_name and string.match(SHORT_SRC, script_name) then
                    f = k
                    break
                end
            end
        end
    end
    return f
end

        MainSection:NewToggle("Large Head Hitbox", "toggles hitboxes", function(state)
    if state then
            for i, v in ipairs(game.Players:GetChildren()) do
                if v.Name == game.Players.LocalPlayer.Name then
                    continue
                end
                v.Character.Head.Size = Vector3.new(4.7, 4.7, 4.7)
                v.Character.Head.Material = "Neon"
                v.Character.Head.BrickColor = BrickColor.new("Really red")
                v.Character.Head.Transparency = 0.7
            end

    else
        for i, v in ipairs(game.Players:GetChildren()) do
            if v.Name == game.Players.LocalPlayer.Name then
                continue
            end
            v.Character.Head.Size = Vector3.new(1, 1, 1)
            v.Character.Head.BrickColor = BrickColor.new("Tan")
            v.Character.Head.Transparency = 0
         end
    end
end)

