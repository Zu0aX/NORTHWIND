local Main = Window:NewTab("ESPs")
        local ESPSection = Main:NewSection("All ESPs")
 ESPSection:NewToggle("Player ESP", "ToggleInfo", function(state)
    if state then

loadstring(game:HttpGet('https://raw.githubusercontent.com/Igor2387/NORTHWIND/refs/heads/main/playeresp'))()

wait()
        for i, v in pairs(game.workspace:GetDescendants()) do
	if v.Name == "GetReal" then
		v.FillTransparency = 0.5
		v.OutlineTransparency = 0
	end
end

    else

        for i, v in pairs(game.workspace:GetDescendants()) do
	if v.Name == "GetReal" then
		v.FillTransparency = 1
		v.OutlineTransparency = 1
	end
end

wait(0.1)
game:GetService("CoreGui").ESP:Destroy()
wait(0.1)
game:GetService("CoreGui").ESP:Destroy()
    end
end)


ESPSection:NewButton("Ore ESP", "Rock ESP", function()


while true do
   list = {}
for i,v in pairs(game:GetDescendants()) do
   if (v.Name):match("deposit") then
   BillboardGui = Instance.new("BillboardGui")
   local Frame = Instance.new("Frame")
   BillboardGui.Parent = v.Rock
   BillboardGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
   BillboardGui.Active = true
   BillboardGui.AlwaysOnTop = true
   BillboardGui.LightInfluence = 1.000
   BillboardGui.Size = UDim2.new(0, 50, 0, 50)
   Frame.Parent = BillboardGui
   Frame.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
   Frame.BackgroundTransparency = 0.2
   Frame.Size = UDim2.new(0, 2, 0, 2)
    local c = Instance.new('TextLabel',Frame)
    c.Size = UDim2.new(2,0,2,0)
    c.BorderSizePixel = 0
    c.TextSize = 15
	c.Font = ("SourceSansLight")
    c.Text = v.Name
    c.TextColor3 = Color3.fromRGB(245, 205, 48)
    v.Rock.BrickColor = BrickColor.new("Bright green")
    v.Rock.Material = Enum.Material.Neon
    c.BackgroundTransparency = 1
    table.insert(list, BillboardGui)
end
end
end
end)
