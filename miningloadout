wait()
game:service"NetworkClient".ChildRemoved:Connect(function()
       game:service"TeleportService":Teleport(game.PlaceId);
end)
wait()
local notif = game:GetService("StarterGui")
notif:SetCore("SendNotification", {
    Title = "Notification",
    Text = "Script Executed - Loading GUI",
    Duration = 10

})

